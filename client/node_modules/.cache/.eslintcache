bVitals.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/configureStore.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/MainComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/ForoComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/StaffComponent.js",["107","108","109"],"import {React, useState} from 'react';\nimport { Breadcrumb, BreadcrumbItem, Card, CardHeader, CardBody} from 'reactstrap';\nimport { Link } from 'react-router-dom';\n//using axios to make API request\nimport Axios from 'axios'\n\nfunction Staff() {\n\n    // add employee states\n    const [role, setRole] = useState('');\n    const [name, setName] = useState('');\n    const [country, setCountry] = useState('');\n    const [salary, setSalary] = useState(0);\n    // show employee list state\n    const[employeeList, setEmployeeList] = useState([]);\n    // update salary state\n    const[newSalary, setNewSalary] = useState(0);\n\n    //add employee function\n    const addEmployee = () => {\n        Axios.post('http://localhost:3001/create', {\n            role: role,\n            name: name, \n            country: country,\n            salary: salary,\n        }).then(() => {\n            alert(\"Empleado agregado\");\n            setEmployeeList([...employeeList, {\n                role: role,\n                name: name, \n                country: country,\n                salary: salary,\n            },]);\n        });\n    };\n\n    //show all employees function\n    const getEmployees = () => {\n        Axios.get('http://localhost:3001/employee').then((response) => {\n            setEmployeeList(response.data)\n        });   \n    };\n\n    //update salary function\n    const updateEmployeeSalary = (id) => {\n        Axios.put('http://localhost:3001/update', {\n            salary: newSalary,\n            id: id\n        }).then((response) => {\n            console.log(response);\n            alert(\"Salario modificado\");\n            setEmployeeList(employeeList.map((val) => {\n                return val.id === id ? {\n                    id: val.id,\n                    name: val.name,\n                    country: val.country,\n                    salary: newSalary,\n                } : val;\n            }));\n        });   \n    };\n\n    // delete employee function\n    const deleteEmployee = (id) => {\n        Axios.delete(`http://localhost:3001/delete/${id}`).then((response) => {\n            console.log(response);\n            alert(\"Empleado eliminado\");\n            setEmployeeList(employeeList.filter((val) => {\n                return val.id !== id;\n            }));\n        });   \n    };\n\n    return(\n        <div className=\"container\">\n            <div className=\"row\">\n                <Breadcrumb>\n                    <BreadcrumbItem><Link to=\"/home\">Inicio</Link></BreadcrumbItem>\n                    <BreadcrumbItem active>Staff</BreadcrumbItem>\n                </Breadcrumb>\n                <div className=\"col-12\">\n                    <h3>Staff</h3>\n                    <hr />\n                </div>                \n            </div>\n\n            {/* Roles div\n            <div className=\"row row content\">\n                <div className=\"col-12 col-md-5\">\n                        <Card>\n                            <CardHeader className=\"bg-primary text-white\">Roles</CardHeader>\n                            <CardBody>\n                                <dl className=\"row p-1\">\n                                    <dt className=\"col-6\">1</dt>\n                                    <dd className=\"col-6\">CEO</dd>\n                                    <dt className=\"col-6\">2</dt>\n                                    <dd className=\"col-6\">CTO</dd>\n                                    <dt className=\"col-6\">3</dt>\n                                    <dd className=\"col-6\">Asistente</dd>\n                                    <dt className=\"col-6\">4</dt>\n                                    <dd className=\"col-6\">Segundo Cheff</dd>\n                                </dl>\n                            </CardBody>\n                        </Card>\n                        <hr />\n                </div>\n            </div> */}\n            \n\n            <div className=\"information\">\n                <label>Cargo:</label>\n                <input type=\"text\" onChange={(event) => {setRole(event.target.value);}}/>\n                <label>Nombre:</label>\n                <input type=\"text\" onChange={(event) => {setName(event.target.value);}}/>\n                <label>País:</label>\n                <input type=\"text\" onChange={(event) => {setCountry(event.target.value);}}/>\n                <label>Salario:</label>\n                <input type=\"number\" onChange={(event) => {setSalary(event.target.value);}}/>\n                <button onClick={addEmployee}>Añadir Empleado</button>\n            </div>\n            <div className=\"employees\">\n                <button onClick={getEmployees}>Mostrar Empleados</button>\n                {employeeList.map((val, key) => {\n                    return (\n                        <div className=\"employee\">\n                            <div>\n                                <h3>Cargo: {val.role}</h3>\n                                <h3>Nombre: {val.name}</h3>\n                                <h3>País: {val.country}</h3>\n                                <h3>Salario {val.salary}</h3>\n                            </div>\n                            <div>\n                                <input type=\"text\" placeholder=\"$2000...\" onChange={(event) => {setNewSalary(event.target.value);}}/>\n                                <button onClick={() => {updateEmployeeSalary(val.id);}}>{\" \"}Modificar salario</button>\n                                <button onClick={() => {deleteEmployee(val.id);}}>Eliminar Empleado</button>\n                            </div>\n                        </div>\n                    );\n                })}\n            </div>\n        </div>\n    );\n}\n\nexport default Staff;    ","/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/promotions.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/leaders.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/MenuComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/forms.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/HomeComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/dishes.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/comments.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/AboutComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/FooterComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/HeaderComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/ContactComponent.js",[],["110","111"],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/ActionCreators.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/components/LoadingComponent.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/redux/ActionTypes.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/shared/leaders.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/shared/dishes.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthyplace/client/src/shared/promotions.js",[],"/Users/javiercanto/Desktop/Web cursos/React JS/healthypl